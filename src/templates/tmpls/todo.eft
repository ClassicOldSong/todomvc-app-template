>li.todo
	>div.view
		>input.toggle
			#type = checkbox
			%checked = {{completed}}
		>label
			@dblclick = edit
			.{{title}}
		>button.destroy
			@click = destroy
	>input.edit#edit
		%value = {{update}}
		@keydown.13.prevent = confirm:{{update}}
		@keydown.27 = cancel:{{title}}
		@blur = confirm:{{update}}
