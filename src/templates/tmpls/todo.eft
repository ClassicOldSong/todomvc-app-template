>li.todo.{{completed}}
	>div.view
		>input.toggle
			#type = checkbox
			%checked = {{stored.completed}}
		>label
			@dblclick = edit
			.{{stored.title}}
		>button.destroy
			@click = destroy
	>input.edit#edit
		%value = {{stored.update}}
		@keydown.13.prevent = blur
		@keydown.27 = cancel:{{stored.title}}
		@blur = confirm:{{update}}
